<html>
  <head>
    <meta charset='utf-8' />
    <title>{{htmlWebpackPlugin.options.title}}</title>
    <meta
      name='description'
      content='{{htmlWebpackPlugin.options.description}}'
    />
    <meta name='viewport' content='width=device-width, initial-scale=1' />
    <link rel='preconnect' href='https://fonts.googleapis.com' />
    <link rel='preconnect' href='https://fonts.gstatic.com' crossorigin />
    <link
      href='https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Roboto+Condensed:ital,wght@0,300;0,400;0,700;1,300;1,400;1,700&display=swap'
      rel='stylesheet'
    />
  </head>
  <body>
    <section class='header'>
      <nav class='header__nav'>
        <div class='header__nav--img'>
          <img src='./img/Breaking-Bad-Logo.jpg' alt='Breaking Bad Logo' />
        </div>
        <div class='header__nav--menu'>
          <ul class='menu-list'>
            <li><a href='' id="CharacterCounter">Characters</a></li>
            <li><a href=''>Episodes</a></li>
            <li><a href=''>Downloads</a></li>
          </ul>
        </div>
      </nav>
    </section>
    <section class='homepage'>

    </section>
    <section class='section-comments'>
      <div class="general__container">
        <button class="close">X</button>
        <div>
        <img class="photo__breaking" src="" alt="">
        </div>
        <h3 class="name__space"></h3>
        <div class="main__content">
          <div class="content__1">
            <p class="one"></p>
            <p class="two"></p>
          </div>
          <div class="content__2">
            <p class="three"></p>
            <p class="four"></p>
          </div>
      </div>
      <h4 class="name__comment"></h4>
      <div class="content__3">
        <ul class="list__comment">
        </ul>
      </div>
      <h5 class="name__add"></h5>
      <div class="input__main">
        <input class="input__text" type="text" placeholder="your name" required>
        <input class="input__mail" type="email" name="" id="" placeholder="massage">
        {{!-- <button type="button" class="button__comment">Comment</button> --}}
      </div>
    </div>
    </section>
      <section class='section-reservations'>
        <div class="main__container">
          <button class="close__but">X</button>
          <div>
          <img class="photo__reserv" src="" alt="">
          </div>
          <h3 class="name__space__reserv"></h3>
          <div class="main__content_reserv">
            <div class="content__one">
              <p class="one_one"></p>
              <p class="two_two"></p>
            </div>
            <div class="content__two">
              <p class="three_three"></p>
              <p class="four_four"></p>
            </div>
        </div>
        <h4 class="name__reserv"></h4>
        <div class="content__three">
          <ul class="list__comment_reserv">
          </ul>
        </div>
        <h5 class="name__add_reserv">chauuuuu</h5>
        <div class="input__main_reserv">
          <input class="text__main" type="text" placeholder="Your name" required>
          <input class="main__mail" type="date" name="" id="" placeholder="Start name">
          <input type="date" name="" id="" class="end__name" placeholder="End name">
        </div>
      </div>
      </section>
    

    <footer class='footer'>
      <div class='footer__container'>
        <p>
          Copyright 2022
        </p>
        <p>
          Erick Hans Martinez - Lucca Micale - Alexandre Medina Sales
        </p>
      </div>
    </footer>
    <script
      src='https://kit.fontawesome.com/219d851abe.js'
      crossorigin='anonymous'
    ></script>
  </body>
</html>

